{
  "name": "francken-consumption-counter",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.18.1",
    "npm": "8.19.2"
  },
  "imports": {
    "#*": "./*"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.2.24",
    "@types/react-calendar-heatmap": "^1.6.4",
    "@types/react-dom": "^18.2.8",
    "axios": "^1.5.1",
    "bootstrap": "^4.6.0",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mutationobserver-shim": "^0.3.7",
    "react": "^18.2.0",
    "react-calendar-heatmap": "^1.9.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-moment": "^1.1.3",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.0.8",
    "typescript": "^4.9.0",
    "usehooks-ts": "^2.9.1",
    "victory": "^36.6.11"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --single-quote --write"]
  },
  "scripts": {
    "start": "parcel src/index.html -p 3000",
    "build": "parcel build src/index.html",
    "test:tsc": "tsc --noEmit",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint './src/**/*.{ts,tsx,js,jsx}' --fix",
    "start-mockapi": "json-server --watch db.json --port 3001",
    "prestart-mockapi": "npm run generate-mock-data",
    "generate-mock-data": "node ./generateMockData",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "coverage": "npm run test -- --coverage --watchAll=false",
    "cypress": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "printWidth": 90,
    "bracketSpacing": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.9.3",
    "@parcel/transformer-webmanifest": "^2.9.3",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "coveralls": "^3.1.1",
    "cypress": "^13.3.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest-plugin-clock": "^2.9.0",
    "miragejs": "^0.1.47",
    "msw": "^1.3.2",
    "parcel": "^2.9.3",
    "parcel-resolver-ts-base-url": "^1.3.1",
    "prettier": "3.0.3"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}", "!src/test-utils/**"],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "msw": {
    "workerDirectory": "dist"
  }
}
